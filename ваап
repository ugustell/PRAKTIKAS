import random

def generate_random_numbers(n, lower_bound, upper_bound):
    random_numbers = [random.randint(lower_bound, upper_bound) for _ in range(n)]
    return random_numbers

if __name__ == "__main__":
    count = 10  # Количество случайных чисел
    lower = 1   # Нижняя граница
    upper = 100 # Верхняя граница
    
    random_numbers = generate_random_numbers(count, lower, upper)
    print("Случайные числа:", random_numbers)
